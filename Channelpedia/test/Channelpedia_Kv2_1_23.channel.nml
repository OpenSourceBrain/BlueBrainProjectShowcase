            <DerivedVariable name="t" dimension="none" exposure="t" value="(100.000/(1+exp(((V -(-46.560))/(44.140))))) * TIME_SCALE"/>
            <DerivedVariable name="V" dimension="none" value="v / VOLT_SCALE"/>
