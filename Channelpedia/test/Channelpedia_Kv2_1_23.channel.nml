                        </rdf:Bag>
                    </bqmodel:isDerivedFrom>
                    <bqmodel:isDescribedBy>
                        <rdf:Bag>
                            <!-- Channelpedia channel ID: 9; link to main page for channel -->
                            <rdf:li rdf:resource="http://channelpedia.epfl.ch/ionchannels/9"/>
                        </rdf:Bag>
                    </bqmodel:isDescribedBy>
