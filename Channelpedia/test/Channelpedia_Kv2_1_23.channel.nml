    <ComponentType name="Channelpedia_Kv2_1_23_h_inf" extends="baseVoltageDepVariable">
        <Constant name = "TIME_SCALE" value = "1 ms" dimension = "time"/>
        <Constant name = "VOLT_SCALE" value = "1 mV" dimension = "voltage"/>
        <Dynamics>
            <DerivedVariable name="x" dimension="none" exposure="x" value="1/(1+exp(((V -(-19.000))/(5.000))))"/>
            <DerivedVariable name="V" dimension="none" value="v / VOLT_SCALE"/>
        </Dynamics>
    </ComponentType>


    <ComponentType name="Channelpedia_Kv2_1_23_m_inf" extends="baseVoltageDepVariable">
        <Constant name = "TIME_SCALE" value = "1 ms" dimension = "time"/>
        <Constant name = "VOLT_SCALE" value = "1 mV" dimension = "voltage"/>
