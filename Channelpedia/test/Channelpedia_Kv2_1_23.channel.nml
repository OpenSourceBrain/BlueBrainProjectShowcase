            <DerivedVariable name="x" dimension="none" exposure="x" value="1/(1+exp(((V -(-9.200))/(-6.600))))"/>
            <DerivedVariable name="V" dimension="none" value="v / VOLT_SCALE"/>
        </Dynamics>
    </ComponentType>


    <ComponentType name="Channelpedia_Kv2_1_23_m_tau" extends="baseVoltageDepTime">
